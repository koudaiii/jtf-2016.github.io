  <div class="section">
    <div class="section-1">
      <div class="container">
        <div class="section-cell inner-news">
          <div class="section-cell-inner">
            <div class="row">
              <div class="col-md-12">
                <div class="col-md-3 col-sm-3 col-xs-12">
                  <img src="assets/img/news.png" alt="news" class="sectioncapt-img">
                </div>
                <div class="col-md-9 col-sm-9 col-xs-12">
                  <h1><%= data.index.news.title %></h1>
                  <!-- blog part begin-->
                  <div class="row">
                    <div class="col-md-12">
                      <div id="postlist">
                        <% data.index.news.contents.each_slice(2) do |news| %>
                        <% news.each do |news| %>
                        <div class="panel">
                          <div class="panel-heading">
                            <div class="text-center">
                              <div class="row">
                                <div class="col-sm-6 col-xs-9">
                                  <h3 class="pull-left"><%= news.title %></h3>
                                </div>
                                <div class="col-sm-6 col-xs-3">
                                  <h4 class="pull-right">
                                    <small><%= news.date %></small>
                                  </h4>
                                </div>
                              </div>
                            </div>
                          </div>
                          <div class="panel-body">
                            <%= news.description %>
                          </div>
                          <div class="panel-footer">
                            <% news.tag.each do |tag| %>
                            <span class="label label-default"><%= tag %></span>
                            <% end %>
                          </div>
                        </div>
                        <% end %>
                        <% end %>
                      </div>
                    </div>
                  </div>
                  <!-- blog part end-->
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
